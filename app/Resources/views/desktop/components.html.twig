                                <h5 class="modal-title" id="ramsModalLabel">Wybierz pamięć ram z listy</h5>
                            </div>
                            <div class="modal-body">
                                <div id="div-1" class="body">
                                    <!--Begin Datatables-->
                                    <div class="row">
                                        <div class="col-lg-12">
                                            <div class="box">
                                                <div id="collapse5" class="body">
                                                    <table
                                                            class="datatable table table-bordered table-condensed table-hover table-striped">
                                                        <thead>
                                                        <tr>
                                                            <th>Nazwa</th>
                                                            <th>Model</th>
                                                            <th>Seria</th>
                                                            <th>Producent</th>
                                                            <th>Pojemność w Gb</th>
                                                            <th>Typ</th>
                                                            <th>Taktowanie</th>
                                                            <th>CAS opóźnienie</th>
                                                        </thead>
                                                        <tbody>
                                                        {% for desktopRam in desktopRams %}
                                                            <tr onclick='location.href = "{{ path('computer_connect_ram', { 'type' : computer.formFactor, 'id': computer.id,  'ram': desktopRam.id }) }}"'>
                                                                <td>{{ desktopRam.name }}</td>
                                                                <td>{{ desktopRam.model }}</td>
                                                                <td>{{ desktopRam.series }}</td>
                                                                <td>{{ desktopRam.brand }}</td>
                                                                <td>{{ desktopRam.capacity }} GB</td>
                                                                <td>{{ desktopRam.type }}</td>
                                                                <td>{{ desktopRam.busSpeed }}</td>
                                                                <td>CL{{ desktopRam.casLatency }}</td>
                                                            </tr>
                                                        {% endfor %}
                                                        </tbody>
                                                    </table>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <!-- /.row -->
                                    <!--End Datatables-->
                                </div>
                                <div class="modal-footer">
                                    <a href="{{ path('desktopram_new', {'computer' : computer.id}) }}"
                                       class="btn btn-warning btn-grad control-label">
                                        <i class="fa fa-plus"></i>
                                        <span>Dodaj nowy</span>
                                    </a>
                                    <button type="button" class="btn btn-primary btn-grad control-label"
                                            data-dismiss="modal">Zamknij
                                    </button>
                                </div>

                            </div>
                        </div>
                    </div>
                </div>

                <!-- Modal gpus-->
                <div class="modal fade" id="gpus" tabindex="-1" role="dialog" aria-labelledby="gpusModalLabel"
                     aria-hidden="true">
                    <div class="modal-dialog modal-lg">
                        <div class="modal-content">

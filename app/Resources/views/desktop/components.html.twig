                                                <div id="collapse4" class="body">
                                                    <table
                                                            class="datatable table table-bordered table-condensed table-hover table-striped">
                                                        <thead>
                                                        <tr>
                                                            <th>Nazwa</th>
                                                            <th>Model</th>
                                                            <th>Seria</th>
                                                            <th>Producent</th>
                                                            <th>Pojemność GB</th>
                                                            <th>Format</th>

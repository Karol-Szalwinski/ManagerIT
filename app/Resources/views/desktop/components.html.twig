                            </div>
                            <div class="modal-body">
                                <div id="div-1" class="body">
                                    <!--Begin Datatables-->
                                    <div class="row">
                                        <div class="col-lg-12">
                                            <div class="box">
                                                <div id="collapse4" class="body">
                                                    <table
                                                            class="datatable table table-bordered table-condensed table-hover table-striped">
                                                        <thead>
                                                        <tr>
                                                            <th>Nazwa</th>
                                                            <th>Model</th>
                                                            <th>Seria</th>

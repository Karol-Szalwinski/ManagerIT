                                        <div class="col-lg-12">
                                            <div class="box">
                                                <div id="collapse4" class="body">
                                                    <table
                                                            class="datatable table table-bordered table-condensed table-hover table-striped">
                                                        <thead>
                                                        <tr>
                                                            <th>Nazwa</th>
                                                            <th>Chipset</th>
                                                            <th>Seria</th>
                                                            <th>Marka</th>
                                                            <th>Pamięć</th>
                                                            <th>Typ pamięci</th>
                                                            <th>Porty</th>
                                                            <th>Typ grafiki</th>
                                                        </thead>
                                                        <tbody>
                                                        {% for gpu in gpus %}
                                                            <tr onclick='location.href = "{{ path('computer_connect_gpu', { 'type' : computer.formFactor, 'id': computer.id,  'gpu': gpu.id }) }}"'>
                                                                <td>{{ gpu.name }}</td>
                                                                <td>{{ gpu.chipset }}</td>
                                                                <td>{{ gpu.series }}</td>
                                                                <td>{{ gpu.brand }}</td>
                                                                <td>{{ gpu.memorySize }}</td>
                                                                <td>{{ gpu.memoryType }}</td>
                                                                <td>{{ gpu.compatiableSlotPort }}</td>
                                                                <td>{{ gpu.formFactor }}</td>
                                                            </tr>
                                                        {% endfor %}
                                                        </tbody>
                                                    </table>
                                                </div>
                                            </div>
                                        </div>
                                    </div>
                                    <!-- /.row -->
                                    <!--End Datatables-->
                                </div>
                                <div class="modal-footer">
                                    <a href="{{ path('gpu_new', {'computer' : computer.id}) }}"
                                       class="btn btn-warning btn-grad control-label">
                                        <i class="fa fa-plus"></i>
                                        <span>Dodaj nowy</span>
                                    </a>
                                    <button type="button" class="btn btn-primary btn-grad control-label"
                                            data-dismiss="modal">Zamknij
                                    </button>
                                </div>

                            </div>
                        </div>

                    </div>
                </div>

                <!-- Modal hdds-->
                <div class="modal fade" id="hdds" tabindex="-1" role="dialog" aria-labelledby="hddsModalLabel"
                     aria-hidden="true">
                    <div class="modal-dialog modal-lg">
                        <div class="modal-content">
                            <div class="modal-header">
                                <button type="button" class="close" data-dismiss="modal"><span
                                            aria-hidden="true">&times;</span><span class="sr-only">Zamknij</span>
                                </button>
                                <h5 class="modal-title" id="hddsModalLabel">Wybierz dysk hdd z listy</h5>
                            </div>
                            <div class="modal-body">
                                <div id="div-1" class="body">
                                    <!--Begin Datatables-->
                                    <div class="row">
                                        <div class="col-lg-12">
                                            <div class="box">
                                                <div id="collapse4" class="body">
                                                    <table
                                                            class="datatable table table-bordered table-condensed table-hover table-striped">
                                                        <thead>
                                                        <tr>
                                                            <th>Nazwa</th>
                                                            <th>Model</th>
                                                            <th>Seria</th>
                                                            <th>Producent</th>
                                                            <th>Pojemność GB</th>
                                                            <th>Format</th>
                                                            <th>Interfejs</th>

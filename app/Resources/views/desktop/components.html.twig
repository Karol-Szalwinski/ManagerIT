                                                            <th>Cache MB</th>
                                                        </thead>
                                                        <tbody>
                                                        {% for hdd in hdds %}
                                                            <tr onclick='location.href = "{{ path('computer_connect_hdd', { 'type' : computer.formFactor, 'id': computer.id,  'hdd': hdd.id }) }}"'>
                                                                <td>{{ hdd.name }}</td>
                                                                <td>{{ hdd.model }}</td>
                                                                <td>{{ hdd.series }}</td>
                                                                <td>{{ hdd.brand }}</td>
                                                                <td>{{ hdd.capacity }}</td>
                                                                <td>{{ hdd.formFactor }}</td>
                                                                <td>{{ hdd.interface }}</td>

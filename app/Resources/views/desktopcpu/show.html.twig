{% extends 'base.html.twig' %}

{% block body %}
    <h1>Desktopcpu</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ desktopCPU.id }}</td>
            </tr>
            <tr>
                <th>Name</th>
                <td>{{ desktopCPU.name }}</td>
            </tr>
            <tr>
                <th>Manufacturer</th>
                <td>{{ desktopCPU.manufacturer }}</td>
            </tr>
            <tr>
                <th>Basefrequency</th>
                <td>{{ desktopCPU.baseFrequency }}</td>
            </tr>
            <tr>
                <th>Maxturbofrequency</th>
                <td>{{ desktopCPU.maxTurboFrequency }}</td>
            </tr>
            <tr>
                <th>Numberofcores</th>
                <td>{{ desktopCPU.numberOfCores }}</td>
            </tr>
            <tr>
                <th>Numberofthreads</th>
                <td>{{ desktopCPU.numberOfThreads }}</td>
            </tr>
            <tr>
                <th>Cashemb</th>
                <td>{{ desktopCPU.casheMB }}</td>
            </tr>
            <tr>
                <th>Tdp</th>
                <td>{{ desktopCPU.tDP }}</td>
            </tr>
            <tr>
                <th>Gpu</th>
                <td>{{ desktopCPU.gPU }}</td>
            </tr>
            <tr>
                <th>Lithographynm</th>
                <td>{{ desktopCPU.lithographyNM }}</td>
            </tr>
            <tr>
                <th>Launchdate</th>
                <td>{% if desktopCPU.launchDate %}{{ desktopCPU.launchDate|date('Y-m-d') }}{% endif %}</td>
            </tr>
            <tr>
                <th>Sockets</th>
                <td>{{ desktopCPU.sockets }}</td>
            </tr>
            <tr>
                <th>Generation</th>
                <td>{{ desktopCPU.generation }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('desktopcpu_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('desktopcpu_edit', { 'id': desktopCPU.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
